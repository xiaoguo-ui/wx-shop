/**app.wxss**/
/* 全局样式iconfont文件 */
@import "./styles/iconfont.wxss";

/* 基本样式的初始化 */
page, view, swiper, swiper-item, image, navgator {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
page {
  /* 定义主题颜色 */
  --themeColor: #eb4450;
  /* 定义统一字体大小 */
  font-size: 28rpx;
}

image {
  width: 100%;
}
